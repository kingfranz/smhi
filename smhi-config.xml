<smhi-config>
	<image-dir>resources/</image-dir>
	<location>
		<latitude>58.786869</latitude>
		<longitude>14.265020</longitude>
	</location>
	<smhi>
		<weather-url>http://opendata-download-metfcst.smhi.se</weather-url>
		<radar-url>http://opendata-download-radar.smhi.se/api/version/latest/area/sweden/product/comp/latest.png</radar-url>
		<category>pmp2g</category>
		<version>2</version>
	</smhi>
	<radar-image>
		<radar-sub-width>112</radar-sub-width>
		<radar-sub-height>45</radar-sub-height>
		<radar-sub-upper-left-x>110</radar-sub-upper-left-x>
		<radar-sub-upper-left-y>595</radar-sub-upper-left-y>
		<radar-interval-minutes>5</radar-interval-minutes>
		<radar-fps>10</radar-fps>
		<radar-ani-hours>5</radar-ani-hours>
		<radar-ani-delay-sec>2</radar-ani-delay-sec>
	</radar-image>
	<graph>
		<max-rain-level>3</max-rain-level>
		<graph-days>7</graph-days>
		<temp-range>
			<min>-20</min>
			<max>30</max>
		</temp-range>
		<wind-style>
			<foreground>white</foreground>
			<background>lightgray</background>
			<stroke>1</stroke>
		</wind-style>
		<rain-style>
			<foreground>blue</foreground>
			<background>red</background>
			<stroke>1</stroke>
		</rain-style>
		<cloud-style>
			<foreground>lightgreen</foreground>
			<background>lightgreen</background>
			<stroke>10</stroke>
		</cloud-style>
		<temp-style>
			<foreground>red</foreground>
			<background>red</background>
			<stroke>8</stroke>
		</temp-style>
	</graph>
	<axis>
		<axis-style>
			<foreground>white</foreground>
			<background>white</background>
			<stroke>2</stroke>
		</axis-style>
		<day-tick-style>
			<foreground>white</foreground>
			<background>white</background>
			<stroke>1</stroke>
		</day-tick-style>
		<font>ARIAL-BOLD-20</font>
		<temp-axis-txt-style>
			<foreground>red</foreground>
			<background>white</background>
			<stroke>1</stroke>
		</temp-axis-txt-style>
		<wind-axis-txt-style>
			<foreground>gray</foreground>
			<background>white</background>
			<stroke>1</stroke>
		</wind-axis-txt-style>
		<rain-axis-txt-style>
			<foreground>blue</foreground>
			<background>white</background>
			<stroke>1</stroke>
		</rain-axis-txt-style>
		<zero-line-style>
			<foreground>white</foreground>
			<background>white</background>
			<stroke>1</stroke>
		</zero-line-style>
		<text-circle-style>
			<foreground>white</foreground>
			<background>white</background>
			<stroke>1</stroke>
		</text-circle-style>
		<label-background>white</label-background>
	</axis>
</smhi-config>
